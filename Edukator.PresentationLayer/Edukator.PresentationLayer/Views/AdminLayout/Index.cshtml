@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Html.PartialAsync("PartialHead.cshtml")

<body>
    <div class="container-xxl position-relative bg-white d-flex p-0">
        <!-- Spinner Start -->
        @await Html.PartialAsync("PartialSpinner.cshtml")
        <!-- Spinner End -->
        <!-- Sidebar Start -->
       @await Html.PartialAsync("PartialSideBar.cshtml")
        <!-- Sidebar End -->
        <!-- Content Start -->
        <div class="content">
            <!-- Navbar Start -->
            @await Html.PartialAsync("PartialNavbar.cshtml")
            <!-- Navbar End -->
            <!-- Blank Start -->
            <div class="container-fluid pt-4 px-4">
                <div class="row vh-100 bg-light rounded align-items-center justify-content-center mx-0">
                    <div class="col-md-6 text-center">
                        @RenderBody()
                    </div>
                </div>
            </div>
            <!-- Blank End -->
            <!-- Footer Start -->
            @await Html.PartialAsync("PartialFooter.cshtml")
            <!-- Footer End -->
        </div>
        <!-- Content End -->
        <!-- Back to Top -->
        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
    </div>

    <!-- JavaScript Libraries -->
   @await Html.PartialAsync("PartialScript.cshtml")
    <!-- Template Javascript -->
    
</body>

</html>